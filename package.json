        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.0",
        "ejs": "^3.1.7",
        "express": "^4.17.3",
        "livereload": "^0.9.3",
        "pg": "^8.7.3",
        "pg-hstore": "^2.3.4",
        "redis": "^4.0.4",
        "sequelize": "^6.17.0",
        "socket.io": "^4.4.1",
        "swagger": "^0.7.5",
        "swagger-jsdoc": "^6.1.0",
        "swagger-ui-express": "^4.3.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "eslint": "^8.10.0",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^7.0.0",
        "jest": "^27.5.1",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "pretty-quick": "^3.1.3",
        "startapp-node": "^1.0.0"
    }
