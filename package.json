{
    "name": "phantombackend",
    "version": "1.0.0",
    "description": "The backend for the phantom project testing handling of request and replies",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "test": "jest --passWithNoTests",
        "start": "node src/index.js",
        "dev": "cross-env HTTPS=true nodemon src/index.js --ext *",
        "format:check": "prettier --check ./src/**/*.js",
        "format:write": "prettier --write ./src/**/*.js",
        "lint:check": "eslint ./src/**/*.js",
        "lint:fix": "eslint --fix ./src/**/*.js",
        "prepare": "husky install"
